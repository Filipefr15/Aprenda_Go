package main

import "fmt"

func main() {

	pessoas := map[string][]string{
		"filipe_rodrigues": []string{
			"sei lรก",
		},
		"ayrton_senna": []string{
			"pilotar aviao", "andar de carro",
		},
		"nicole_alves": []string{
			"ver filmes",
		},
	}

	for k, v := range pessoas {
		fmt.Println(k)
		for l, w := range v {
			fmt.Println("\t", l+1, "-", w)
		}
	}

}
